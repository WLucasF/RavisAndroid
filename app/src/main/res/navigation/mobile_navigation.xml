<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_contacts">


    <fragment
        android:id="@+id/navigation_contacts"
        android:name="com.example.ravisandroid.ui.dashboard.ContactsFragment"
        android:label="Contacts"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_calendar"
        android:name="com.example.ravisandroid.ui.home.HomeFragment"
        android:label="Calendar"
        tools:layout="@layout/fragment_dashboard" />
    <fragment
        android:id="@+id/navigation_messages"
        android:name="com.example.ravisandroid.ui.notifications.NotificationsFragment"
        android:label="Messages"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_call"
        android:name="com.example.ravisandroid.ui.notifications.NotificationsFragment"
        android:label="Call"
        tools:layout="@layout/fragment_notifications" />
</navigation>